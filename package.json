{
  "name": "signal-stock-screener",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check": "npm run lint && prettier --write . && tsc --noEmit",
    "seed": "npx supabase db seed",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:serve": "npx supabase functions serve",
    "supabase:status": "npx supabase status",
    "supabase:restart": "npm run supabase:stop && npm run supabase:start",
    "supabase:db-reset": "npx supabase db reset",
    "supabase:db-reset-all": "npm run supabase:db-reset && npm run supabase:generate-types",
    "supabase:generate-types": "npx supabase gen types typescript --local --schema public > ./lib/types/definitions.ts",
    "supabase:generate-migration": "npx supabase db diff | npx supabase migration new",
    "supabase:push": "npx supabase push",
    "supabase:pull": "npx supabase pull"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prepare": "husky",
  "dependencies": {
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@supabase/supabase-js": "^2.39.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.0.0",
    "lucide-react": "^0.525.0",
    "next": "^14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^3.0.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "^14.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
